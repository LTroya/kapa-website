---
import BaseLayout from "@/layouts/BaseLayout.astro";

import Hero from "@/components/pricing/Hero.astro";
import Divider from "../components/ui/Divider.astro";
import Benefits from "@/components/pricing/Benefits.astro";
import AllPricing from "@/components/pricing/AllPricing.astro";
import CallToAction from "@/components/pricing/CallToAction.astro";
import Reviews from "../components/landing/Reviews.astro";
import FAQ from "../components/landing/FAQ.astro";
import PricingClients from "../components/pricing/PricingClients.astro";
---

<BaseLayout>
	<Hero />
	<Divider size="small" />
	<Benefits />
	<Divider size="small" />
	<AllPricing />
	<Divider size="medium" />
	<PricingClients />
	<Divider size="large" />
	<CallToAction />
	<Divider size="large" />
	<Reviews />
	<Divider size="large" />
	<FAQ />
	<Divider size="large" />
</BaseLayout>
